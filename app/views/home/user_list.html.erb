<h1  style="margin: 60px;">All Users</h1>
<table>
	<tr>
		<th>First Name</th>
		<th>Last Name</th>
		<th>Address</th>
    </tr>
    
      <% @user.each do |user|%>
       <tr>
	    <td><%= user.first_name%></td>
	    <td><%= user.last_name%></td>
	    <td><%= user.address%></td>
       </tr>
      <%end%>
    
  </table>    	